/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@views/*": ["./src/views"],
      "@services/*": ["./src/app/services"],
      "@models/*": ["./src/app/models"],
      "@constants/*": ["./src/app/constants"]
    }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
}
